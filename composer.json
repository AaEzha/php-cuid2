{
    "name": "xaevik/cuid2",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": [
        "cuid",
        "identifier"
    ],
    "authors": [
        {
            "name": "Alan Brault",
            "email": "alan.brault@visus.io"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ergebnis/composer-normalize": "^2.29",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.4",
        "phpstan/phpstan": "^1.9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Xaevik\\Cuid2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xaevik\\Cuid2\\Test\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
